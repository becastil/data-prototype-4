{
  "name": "medical-reporting-platform",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "cd apps/web && npx prisma generate",
    "db:push": "cd apps/web && npx prisma db push",
    "db:seed": "cd apps/web && npx prisma db seed",
    "db:studio": "cd apps/web && npx prisma studio",
    "start": "cd apps/web && npm start",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
